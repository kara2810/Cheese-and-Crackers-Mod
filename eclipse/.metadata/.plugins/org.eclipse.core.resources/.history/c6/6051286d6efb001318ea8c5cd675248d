package com.kenny2810.CheeseAndCrackers.Blocks;

import net.minecraft.block.Block;
import net.minecraft.client.renderer.texture.IIconRegister;
import net.minecraft.creativetab.CreativeTabs;

public class BlazeBlock extends Block {

		public static String modid="CheeseAndCrackers";
	    public BlazeBlock()
	    {
	        setBlockTextureName(modid + ":" + "Cracker");
	        this.blockHardness = 10;
	        this.setCreativeTab(CreativeTabs.tabBlock);
	    }
	    public void registerIcons(IIconRegister iconRegister)
	    {
	      itemIcon = iconRegister.registerIcon("CheeseAndCrackers:Cracker");
	    }
	    
	   
	}

}
